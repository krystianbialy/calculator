{
    "extends": ["airbnb-base/legacy", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "default-case": "off",
        "no-eval":"off",
        "no-undef":"off"
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2015,
        "sourceType": "module"
    }
}